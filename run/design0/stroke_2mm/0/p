FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        // type            zeroGradient;
        type            totalPressure;
        gamma           1.4;
        p0              uniform 6.7e3; //[m^2/s^2]
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    valve
    {
        type            zeroGradient;
    }

    walls
    {
        type            zeroGradient;
    }

}

