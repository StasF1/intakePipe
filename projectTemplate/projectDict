# Default case clonning parameters
# ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
DESIGN_NO=2 # max design number from 0

STROKE_INIT=2 #[mm] initial valve stroke

STROKE_FINITE=10 #[mm] finite valve stroke

STROKE_STEP=1 #[mm] stroke step - only int type


# Pipe & valve geometry (required for post-processing only)
# ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
d_pipe=25.688 #[mm] minimal intake pipe diameter - see diameter for A point

d_2Pipe=29.756 #[mm] valve neck chamfer diameter - see diameter for A1 point

d_bar=6.96 #[mm] bar diameter - see d_c

d_1=26.782 #[mm] valve diameter

d_2=29.206 #[mm] head valve diameter

theta=45 #[deg] valve chamfer angle

